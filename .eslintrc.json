{
	"extends": [
		"plugin:@typescript-eslint/recommended",
        "plugin:react/recommended",
		"airbnb-typescript"
	],
    "parser": "@typescript-eslint/parser",
	"parserOptions": {
    	"project": "./tsconfig.json"
	},
    "plugins": [
        "react",
        "@typescript-eslint"
    ],
    "rules": {
      "react/jsx-props-no-spreading": 0
    }
}
