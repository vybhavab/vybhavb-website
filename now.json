{
  "version": 2,
  "name": "vue",
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "routes": [
    {
      "src": "/service-worker.js",
      "headers": {
        "cache-control": "s-maxage=0"
      }
    },
    {
      "handle": "filesystem"
    },
    {
      "src": ".*",
      "dest": "dist/index.html"
    }
  ]
}